<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <clear />
    <!-- Local Granville feed (includes both Granville.Orleans.* and Granville.Rpc.* packages) -->
    <add key="LocalGranville" value="../../../Artifacts/Release" />
    <!-- Default NuGet feed -->
    <add key="nuget" value="https://api.nuget.org/v3/index.json" protocolVersion="3" />
  </packageSources>
  
  <packageSourceMapping>
    <packageSource key="LocalGranville">
      <package pattern="Granville.*" />
      <package pattern="Microsoft.Orleans.*granville-shim" />
    </packageSource>
    <packageSource key="nuget">
      <package pattern="*" />
    </packageSource>
  </packageSourceMapping>
</configuration>

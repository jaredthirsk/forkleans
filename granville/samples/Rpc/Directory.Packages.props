    <PackageVersion Include="Microsoft.Orleans.CodeGenerator" Version="9.1.2" />
    <PackageVersion Include="Microsoft.Orleans.Sdk" Version="9.1.2.148-granville-shim" />
    
    <!-- Use Granville Orleans build tools to match RPC dependencies -->

